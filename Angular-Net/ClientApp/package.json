{
    "name": "client-app",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "echo hello && ng serve --progress=false",
        "build": "ng build",
        "build:scss": "sass projects/client-app/scss:projects/client-app/src/assets/css",
        "build:stage": "ng build --aot true --configuration=stage --build-optimizer --vendor-chunk",
        "build:test": "ng build --aot true --configuration=test --build-optimizer",
        "build:dev": "ng build --aot true --configuration=dev",
        "build:prod": "ng build --configuration=production",
        "lint": "ng lint",
        "minicss": "uglifycss projects/client-app/src/assets/css/sportsbiz.css --ugly-comments --output projects/client-app/src/assets/css/sportsbiz.min.css",
        "test": "ng lint && ng test --browsers ChromeHeadless --watch=false",
        "watch": "ng test --browsers ChromeHeadless --watch --reporters dots",
        "watch:scss": "sass --watch scss:src/assets/css",
        "e2e": "ng e2e",
        "format:write": "prettier ./**/*.{ts,json,md,scss} --write",
        "format:test": "prettier ./**/*.{ts,json,md,scss} --list-different",
        "release": "standard-version",
        "analyze": "npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/client-app/stats.json",
        "postinstall": "ngcc --first-only --create-ivy-entry-points"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-push": "ng build --aot true --configuration=production --build-optimizer --vendor-chunk"
        }
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "private": true,
    "dependencies": {
        "@angular-extensions/elements": "^9.2.0",
        "@angular-extensions/model": "^8.1.0",
        "@angular/animations": "^9.1.6",
        "@angular/cdk": "^9.2.3",
        "@angular/common": "^9.1.6",
        "@angular/compiler": "^9.1.6",
        "@angular/core": "^9.1.6",
        "@angular/forms": "^9.1.6",
        "@angular/localize": "^9.1.6",
        "@angular/material": "^9.1.3",
        "@angular/platform-browser": "^9.1.6",
        "@angular/platform-browser-dynamic": "^9.1.6",
        "@angular/platform-server": "^9.1.6",
        "@angular/router": "^9.1.6",
        "@fortawesome/angular-fontawesome": "^0.6.0",
        "@fortawesome/fontawesome-free": "^5.12.1",
        "@fortawesome/fontawesome-svg-core": "^1.2.27",
        "@fortawesome/free-brands-svg-icons": "^5.12.1",
        "@fortawesome/free-solid-svg-icons": "^5.12.1",
        "@material-extended/mde": "^3.0.0",
        "@ng-bootstrap/ng-bootstrap": "^6.0.1",
        "arrive": "^2.4.1",
        "bootstrap": "^4.4.1",
        "bootstrap-material-design": "^4.1.2",
        "bootstrap-notify": "^3.1.3",
        "browser-detect": "^0.2.28",
        "classlist.js": "^1.1.20150312",
        "express": "^4.17.1",
        "googleapis": "^48.0.0",
        "hammerjs": "^2.0.8",
        "minimist": "^1.2.5",
        "moment": "^2.24.0",
        "ngx-perfect-scrollbar": "^9.0.0",
        "powerbi-client": "^2.14.1",
        "powerbi-models": "^1.5.0",
        "rxjs": "~6.5.4",
        "tslib": "^1.10.0",
        "uuid": "^7.0.2",
        "yargs-parser": "^18.1.3",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.901.4",
        "@angular-eslint/builder": "0.0.1-alpha.32",
        "@angular-eslint/eslint-plugin": "0.0.1-alpha.32",
        "@angular-eslint/eslint-plugin-template": "0.0.1-alpha.32",
        "@angular-eslint/schematics": "^0.0.1-alpha.32",
        "@angular-eslint/template-parser": "0.0.1-alpha.32",
        "@angular/cli": "^9.1.5",
        "@angular/compiler-cli": "^9.1.6",
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^8.3.4",
        "@sb-schematics/angular": "file:../../../../../CodeCommit/nexus/npm/sb-schematics-angular/sb-schematics-angular-1.1.3.tgz",
        "@types/chai": "^4.2.11",
        "@types/cucumber": "^6.0.1",
        "@types/jasmine": "~3.5.0",
